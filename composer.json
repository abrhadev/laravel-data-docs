{
    "name": "abrha/laravel-data-docs",
    "description": "Generate beautiful API documentation from Laravel Data classes with attributes for query parameters, hidden fields, and response data",
    "keywords": [
        "Abrha",
        "laravel",
        "laravel-data",
        "laravel-data-docs",
        "api-documentation",
        "scribe",
        "openapi",
        "swagger",
        "schema-generation",
        "dto"
    ],
    "homepage": "https://github.com/abrhadev/laravel-data-docs",
    "license": "MIT",
    "authors": [
        {
            "name": "Hamed Ehtesham",
            "email": "H4M3D3@abrha.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "fakerphp/faker": "^1.21",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "knuckleswtf/scribe": "^4.0|^5.0",
        "spatie/laravel-data": "^4.14",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Abrha\\LaravelDataDocs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abrha\\LaravelDataDocs\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=256M",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abrha\\LaravelDataDocs\\LaravelDataDocsServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
